---
layout: null
---

{
	"posts" : [
    {% for post in site.posts %}{
      "title": "{{ post.title }}",
      "url": "{{ post.url | prepend: site.baseurl }}",
      "date": "{{ post.date | date: "%B %-d, %Y" }}",
      "raw_date": "{{ post.date }}",
      "id": "{{ post.color }}",
      "description": "{{ post.description }}" 
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}